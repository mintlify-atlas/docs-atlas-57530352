  "keyFeatures": [
    "User authentication and account creation",
    "Browse marketplace feed with item listings",
    "Create and publish sale listings",
    "In-app messaging between buyers and sellers",
    "User profiles with purchase and sales history",
    "Integrated payment options (CashApp, PayPal)",
    "Campus-specific meetup locations",
    "Responsive React-based interface"
  ],
  "publicApiSurface": [
    "LoginServlet - POST /Final_Project/LoginServlet",
    "CreateAccountServlet - POST /Final_Project/CreateAccountServlet",
    "ItemFetchServlet - GET /Final_Project/ItemFetchServlet",
    "AddItemServlet - POST /Final_Project/AddItemServlet",
    "Home Component - Landing page with USC branding",
    "Feed Component - Display marketplace items",
    "CreateSale Component - Form to list new items",
    "PurchaseItem Component - Item purchase interface",
    "Profile Component - User profile with transaction history",
    "Message Component - Messaging interface",
    "Navbar Component - Navigation with authentication state"
